createCorrelationPlots <- function(path, filename, df) {
corrPlot <- ggpairs(
df, densityDiag=list(continuous="density"), axisLabels='show')
png(file.path(path, filename), height=1000, width=1000)
print(corrPlot)
dev.off()
}
